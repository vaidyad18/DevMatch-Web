{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://devmatch-backend.vercel.app/$1" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://devmatch-io.vercel.app" },
        { "key": "Access-Control-Allow-Credentials", "value": "true" }
      ]
    }
  ]
}
